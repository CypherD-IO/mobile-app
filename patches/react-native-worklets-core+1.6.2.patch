+  _jsRuntime = jsRuntime;
+  _jsCallInvoker = jsCallInvoker;
+  _workletCallInvoker = workletCallInvoker;
+  _contextId = ++contextIdNumber;
+
+  _jsThreadId = std::this_thread::get_id();
+  runtimeMappings.emplace(&getWorkletRuntime(), this);
