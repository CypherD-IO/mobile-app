+  if (instance == nullptr) {
+    instance = std::make_shared<JsiWorkletApi>();
+  }
+  return instance;
+}
+
+/**
+ Invalidate the api instance.
+ */
+void JsiWorkletApi::invalidateInstance() { instance = nullptr; }
