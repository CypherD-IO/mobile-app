+  bool _isWorklet = false;
+  std::shared_ptr<JsiWrapper> _closureWrapper;
+  std::string _location = "";
+  std::string _code = "";
+  std::string _name = "fn";
+  std::string _hash;
+  bool _isRea30Compat = false;
+  double _workletHash = 0;
+};
