+void JsiWorkletContext::initialize(
+    const std::string &name, jsi::Runtime *jsRuntime,
+    std::function<void(std::function<void()> &&)> jsCallInvoker,
+    std::function<void(std::function<void()> &&)> workletCallInvoker) {
