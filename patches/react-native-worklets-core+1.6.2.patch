+
+// https://github.com/embeddedartistry/embedded-resources/blob/master/examples/cpp/dispatch.cpp
+namespace RNWorklet {
+
+class DispatchQueue {
+  typedef std::function<void(void)> fp_t;
+
+public:
+  explicit DispatchQueue(std::string name);
+
+  ~DispatchQueue();
+
+  // dispatch and copy
