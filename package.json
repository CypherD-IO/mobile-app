{
  "name": "cypherd-wallet",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "pod-install": "npx pod-install",
    "clean-android": "cd android && ./gradlew clean --refresh-dependencies && cd ..",
    "ios": "react-native run-ios --simulator='iPhone 16 Pro Max'",
    "start": "react-native run-ios --simulator='iPhone 16 Pro Max' && adb devices && react-native run-android",
    "link": "react-native link",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "postinstall": "./node_modules/.bin/rn-nodeify --install 'crypto,buffer,react-native-randombytes,vm,stream,http,https,os,url,fs' --hack&&patch-package",
    "react-devtools": "react-devtools",
    "postversion": "react-native-version",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "e2e:build": "detox build",
    "e2e:test": "detox test",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:test:ios": "detox build --configuration ios.sim.debug && detox test --configuration ios.sim.debug",
    "e2e:test:ios:single": "detox test --configuration ios.sim.debug",
    "e2e:test:ios:dev": "npm run e2e:test:ios",
    "e2e:test:ios:clean": "npm run e2e:reset-sim && npm run e2e:test:ios",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:test:android": "detox test --configuration android.emu.debug",
    "e2e:reset-sim": "bash scripts/reset-sim.sh",
    "e2e:clean-test:ios": "npm run e2e:reset-sim && npm run e2e:test:ios",
    "e2e:clean-test:ios:single": "npm run e2e:reset-sim && npm run e2e:test:ios:single",
    "e2e:prepare": "./scripts/prepare-test-environment.sh",
    "e2e:full-workflow": "npm run e2e:prepare && npm run e2e:clean-test:ios",
    "e2e:onboarding": "detox test --configuration ios.sim.debug e2e/onboardingFlow.test.ts",
    "e2e:onboarding:clean": "npm run e2e:reset-sim && npm run e2e:onboarding",
    "e2e:import": "detox test --configuration ios.sim.debug e2e/importWalletFlow.test.ts",
    "e2e:import:clean": "npm run e2e:reset-sim && npm run e2e:import",
    "e2e:loadcard": "detox test --configuration ios.sim.debug e2e/loadCardFlow.test.ts",
    "e2e:loadcard:clean": "npm run e2e:reset-sim && npm run e2e:loadcard",
    "check-build-readiness": "./scripts/test-build-readiness.sh",
    "pre-commit-check": "./scripts/test-build-readiness.sh"
  },
